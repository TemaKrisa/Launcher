<Page x:Class="Launcher.View.OW"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Launcher.View"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OW">
    <Grid>
        <Grid.RowDefinitions>
        <RowDefinition Height="80"/>
        <RowDefinition/>
    </Grid.RowDefinitions>

    <Image Stretch="Fill" Source="/Assets/Icons/OWBACK.JPG" Grid.RowSpan="2" />

    <Image Stretch="UniformToFill" Source="/Assets/Icons/OWBACK.JPG" >
        <Image.Effect>
            <BlurEffect Radius="62" KernelType="Gaussian"/>
        </Image.Effect>
    </Image>
    </Grid>
</Page>
